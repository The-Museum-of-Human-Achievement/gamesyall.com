# Deployment credentials - Copy this file to .env and fill in the actual values
# NEVER commit the .env file to git!

# Staging credentials
STAGING_HOST="dev.gamesyall.com"
STAGING_USERNAME="your_staging_username"
STAGING_PASSWORD="your_staging_password"
STAGING_PORT=22

# Production credentials
PRODUCTION_HOST="gamesyall.com"
PRODUCTION_PATH="/home/your_username/gamesyall.com/"
PRODUCTION_USERNAME="your_production_username"
PRODUCTION_PASSWORD="your_production_password"
PRODUCTION_PORT=22

# Google Sheets published CSV URL
GAMES_CSV_URL=https://docs.google.com/spreadsheets/d/YOUR_SPREADSHEET_ID/edit?usp=drivesdk&single=true&output=csv
